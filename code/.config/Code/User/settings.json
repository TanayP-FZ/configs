    "cSpell.enableFiletypes": [
        "!haskell"
    ],
    "cSpell.enabled": true,
    "cSpell.spellCheckDelayMs": 1000,
    "workbench.activityBar.visible": true,
    "languageServerHaskell.diagnosticsOnChange": false,
    "languageServerHaskell.formattingProvider": "none",
    "update.mode": "manual",
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "vim.hlsearch": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "rust.clippy_preference": "on",
    "rust.wait_to_build": 1000,
    "vscode-neovim.neovimExecutablePaths.linux": "/home/miles/software/nvim.appimage",
    "editor.cursorBlinking": "solid",
    "testMate.cpp.log.logSentry": "disable_3",
    "testMate.cpp.test.executables": "{bazel-bin,build,Build,BUILD,out,Out,OUT}/**/*{test,Test,TEST}*",
    "testMate.cpp.test.advancedExecutables": [

      {
        "pattern": "bazel-bin/tests/*Test*",
        "sourceFileMap": { "/proc/self/cwd": "${workspaceFolder}" },
      }
    ],
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "extensions.ignoreRecommendations": true,
    "julia.symbolCacheDownload": true,
    "python.linting.pycodestyleArgs": [
        "--ignore=E501"
    ],
    "python.linting.flake8Args": [
        "--ignore=E501"
    ],
    "editor.tabSize": 2,
    "files.associations": {
        "*.env": "shellscript"
    },
    "C_Cpp.intelliSenseEngine": "Disabled",
    "cmake.configureOnOpen": true,
    "python.linting.mypyEnabled": true,
    "python.venvFolders": [
        "todo-use-work-dir-in-some-cases"
    ],
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "workbench.statusBar.visible": false
}
